<SCRIPT LANGUAGE="Javascript" type="text/javascript"><!--

function flip(e){
  var menustyle = eval(getStyleObj(e))
    if (menustyle.display=="none"){
      menustyle.display='';
         if (e == "alertSettings") {
            document.getElementById("registrationOptional").innerHTML = "Hide alert & other settings. <img src=\"/scidirimg/sign_minus.gif\"/>";
            document.getElementById("textRow").style.background = '#f7f7f7';
         }
    }
    else{
      menustyle.display="none"
         if (e == "alertSettings") {
             document.getElementById("registrationOptional").innerHTML = "Show alert & other settings. <img src=\"/scidirimg/sign_plus.gif\"/>";
             document.getElementById("textRow").style.background = '#efefef';
         }
    }
}

function getStyleObj(elem,parent) {
   if (document.layers) {
   if (parent) {
      return "document."+parent+".document."+elem;
   } else {
      return "document."+elem + ".style";
   }
   } else if (document.all) {
     return "document.all."+elem + ".style";
   } else if (document.getElementById) {
     return "document.getElementById('"+elem+"').style";
   }
}
function flipLogin(e,button){
  var t = eval(getStyleObj(e));
  var u = $('loginPlus');
  var v = $('userPlus');
  var userbox = $('userBox');
  var j = $('loginPlusScript');
  if(button == null){
    if (t.display=="none"){
      t.display = 'block';
      j.removeClassName('plus');
      j.addClassName('minus');
    }
    else{
      t.display = 'none';
      j.removeClassName('minus');
      j.addClassName('plus');
    }
  }
  else if (button == "userPlus" ) {
    if (t.display=="none" ){
      t.display = 'block';
      v.removeClassName('userPlus');
      v.addClassName('userMinus');
    }
    else{
      t.display = 'none';
      v.removeClassName('userMinus');
      v.addClassName('userPlus');
    }
  }
  else{
    if (t.display=="none" ){
      t.display = 'block';
      userbox.style.display ='none';
      v.removeClassName('userMinus');
      v.addClassName('userPlus');
    }
    else{
      t.display = 'none';
    }
  }
}


//--></SCRIPT>


