<script type="text/javascript">
    var autoSuggestRunning = false;
    $(
        function () {
            var selectTerm;
            function selectTermf() {
                selectTerm = $('#selectTerm').val();
            }

            $("#selectTerm").change(selectTermf);

            //Adding the click event handler to the search button.
            $('.searchButton').click
            (
                function () {
                    var queryText = $("#divtextSearch").children("input[type=text]").val();
                    var searchScope = $("#ddlGroup").val();
                    var solrSwitch = $("#hfIsSolrSearchEnabled").val();
                    var isSolrSearchEnabled = true;
                    isSolrSearchEnabled = solrSwitch == "1" ? true : false;
                    //Solr Search Switch
                    if (isSolrSearchEnabled) {
                        //Solr is ON: 3rd parameter is "isMobile", set this to false.
                        if (!autoSuggestRunning) {
                            doSolrSearch(queryText, searchScope, false);
                        }
                    } else {
                        //Legacy/Windows Search
                        //Performing the search.
                        doGlobalSearch(queryText, searchScope);
                    }
                }
            );
            return false;
        }
    );

    $(document).ready
    (
        function () {
            function stopRKey(evt) {
                var evt = (evt) ? evt : ((event) ? event : null);
                var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null);
                if ((evt.keyCode == 13) && (node.type == "text")) { return false; }
            };

            document.onkeypress = stopRKey;

            //Adding the click event handler to the search button.
            $("#divtextSearch").children("input[type=text]").keydown
            (
                function (e) {
                    if (e.keyCode == 13) {
                        var queryText = $("#divtextSearch").children("input[type=text]").val();
                        var searchScope = $("#ddlGroup").val();

                        var solrSwitch = $("#hfIsSolrSearchEnabled").val();
                        var isSolrSearchEnabled = true;
                        isSolrSearchEnabled = solrSwitch == "1" ? true : false;

                        //Solr Search Switch
                        if (isSolrSearchEnabled) {
                            //Solr is ON: 3rd parameter is "isMobile", set this to false.
                            doSolrSearch(queryText, searchScope, false);
                        } else {
                            //Legacy/Windows Search
                            //Performing the search.
                            doGlobalSearch(queryText, searchScope);
                        }
                    }
                }
            );

            var maxIE = 8;
            var $select = $('.searchCriteria #ddlGroup');
            var $controls = $('.searchButton, .searchLink');

            (
                function dropdownWidthFixIE(maxIE, $select, $controls) {
                    if ($.browser.msie && $.browser.version <= maxIE) {
                        $select
                            .bind
                            (
                                'focusin click',
                                function () {
                                    $(this).attr('style', 'position:absolute;width:auto;');
                                    $controls.hide();
                                }
                            )
                            .bind
                            (
                                'focusout change',
                                function () {
                                    $(this).attr('style', 'position:relative;');
                                    $controls.show();
                                }
                            );
                    }
                }
            )
            (maxIE, $select, $controls);
        }
    );

    // An undefined onchange function throws error: clearLeftPadding('ddlGroup')
    function clearLeftPadding(str) {
        return true;
    }

    function doGlobalSearch(queryText, searchScope) {
        window.location.href = "/searchresults.aspx?q=" + escape(queryText) + "&t=" + searchScope + "&p=1&s=1&c=0";
    }

</script>
