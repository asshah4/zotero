    <script language="javascript" type="text/javascript">
        var time = 10; //How long (in seconds) to countdown 
        var page = "/index.aspx"; //The page to redirect to 
        function countDown() {
            time--;
            if (time == 0) {
                gett("container").innerHTML = time;
                window.location = page;
            }
            else if (time > 0) {
                gett("container").innerHTML = time;
            }
        }
        function gett(id) {
            if (document.getElementById) return document.getElementById(id);
            if (document.all) return document.all.id;
            if (document.layers) return document.layers.id;
            if (window.opera) return window.opera.id;
        }
        function init() {
            if (gett('container')) {
                setInterval(countDown, 1000);
                gett("container").innerHTML = time;
            }
            else {
                setTimeout(init, 50);
            }
        }
        if (document.URL.indexOf("/cgi/content/full/") != -1 ||
            document.URL.indexOf("/cgi/content/short/") != -1 ||
            document.URL.indexOf("/cgi/content/extract/") != -1) {
            // let all incoming URLs of the Highwire article link form through to the code-behind
        }
        else {
            // all other forms of URL are redirected to index.aspx
            document.onload = init();
        }
    </script>
