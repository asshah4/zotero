<script type="text/javascript">
        /*
         * won't use browser specific css anymore, add browser class in body
         * e.g to add IE specific styling add in beggining of css rule .ie*Body ... {}
         */
        function addUAClass() {
            // when IE6, add ie6Body class in body, when IE7 add class ie7Body
            // in body and when IE >= 8 add class ieBody in body
            if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)) {
                var ieversion = new Number(RegExp.$1);
                if (ieversion >= 8) {
                    document.body.className = document.body.className + ' ieBody';
                } else if (ieversion == 7) {
                    document.body.className = document.body.className + ' ie7Body';
                } else if (ieversion == 6) {
                    document.body.className = document.body.className + ' ie6Body';
                }
            }
            // when FF add ffBody className in body
            if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){ //test for Firefox/x.x or Firefox x.x (ignoring remaining digits);
                document.body.className = document.body.className + ' ffBody';
            }

            // when Chrome add chromeBody className in body
            if (/Chrome[\/\s](\d+\.\d+)/.test(navigator.userAgent)){ //test for Chrome
                document.body.className = document.body.className + ' chromeBody';
            }
        }
    </script></head><body onload="addUAClass();" class="yui3-skin-sam Page404 layout_generic">



