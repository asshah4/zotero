    <script type="text/javascript">
        //  Call the "use" method, passing in "node-menunav".  This will load the
        //  script and CSS for the MenuNav Node Plugin and all of the required
        //  dependencies.
        //  also load module "custommenunavskin" to customize skin of top navigation

        var dict = {
            modules: {
                "custommenunavskin": {
                    type: "css",
                    fullpath: "/templates/jsp/_style4/_ar/css/custommenunav.css",
                   supersedes: ['skin-sam-node-menunav']
               }
           }
        };

        // use these values of dict to change way we retrieve content over https
        // the base is used to load loader-min.js (over https we'll serve it locally)
        if (document.location.protocol == "https:") {
            dict['comboBase'] = '/action/yui/combo?';
            dict['combine'] = true;
            dict['timeout'] = 10000;
            dict['base'] = '/templates/jsp/js/';
        }

        YUI(dict).use("node-menunav", "custommenunavskin", function(Y) {
            Y.on("domready", function () {
                var menu = Y.one('#topNavigation');
                menu.plug(Y.Plugin.NodeMenuNav, { submenuShowDelay: 0, submenuHideDelay: 0 });

                // move hidden submenu of all journals in correct place
                var hiddenSubMenu = Y.one("div#hiddenSubMenu > div:first-child");
                var journalsSubMenu = Y.one("div#topNavigation ul li.yui3-menuitem");
                if(journalsSubMenu != null) {
                    journalsSubMenu.append(hiddenSubMenu);
                }


                // add custom handler on mouse move, since yui widget will try by default to align
                // left hand side of submenu with parent menu's item left hand side, for this submenu we wnat it
                // to display centered under its parent
                var journalsTopMenuItemLabel = Y.one("div#topNavigation ul li.yui3-menuitem a");
                journalsTopMenuItemLabel.on('mousemove', setMultiColStyle);

                function setMultiColStyle() {
                    var multiColumnsList = Y.one("div#topNavigation .multiColumnListWrapper");
                    multiColumnsList.setStyle('left', '-32em')
                    multiColumnsList.setStyle('position', 'absolute');
                    multiColumnsList.setStyle('top', '4px');
                    multiColumnsList.setStyle('width', '85em');
                }

            });

        });
    </script>
   
</div>
</div>

        



